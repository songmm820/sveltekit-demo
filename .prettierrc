{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"tailwindStylesheet": "./src/css/app.css",
	"tailwindFunctions": ["cva", "clsx", "twMerge", "cn"],
	"tailwindPreserveWhitespace": true,
	"tailwindPreserveDuplicates": true,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
